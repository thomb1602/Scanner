<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thomb.scanner">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:name=".Scanner"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PutawayActivity" />
        <activity android:name=".ShowBinsActivity" />
        <activity android:name=".AddingActivities.AddStockActivity" />
        <activity android:name=".AddingActivities.AddStockScanBinOrProductActivity" />
        <activity android:name=".PutawayBinListActivity" />
        <activity android:name=".VerifyBinByScanActivity" />
        <activity android:name=".PutawayBinVerifiedActivity" />
        <activity android:name=".PutawayCompletedActivity" />
        <activity android:name=".AddingActivities.ThenScanBinActivity" />
        <activity android:name=".AddingActivities.AddStockBinVerifiedActivity" />
        <activity android:name=".AddingActivities.AddStockHowManyActivity" />
        <activity android:name=".AddingActivities.AddStockProductIdentifiedActivity" />
        <activity android:name=".AddingActivities.AddStockAdditionVerifiedActivity"></activity>
    </application>

</manifest>